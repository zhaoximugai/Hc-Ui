{
  "name": "@stophjc/hc-ui",
  "version": "1.0.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "description": "Vue3 UI",
  "keywords": [
    "Vue3",
    "UI"
  ],
  "author": "hjc",
  "license": "ISC",
  "sideEffects": [
    "dist/es/index.css"
  ],
  "main": "./dist/es/hc-ui.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/hc-ui.js",
      "require": "./dist/umd/hc-ui.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build-lib build-es build-cjs",
    "preview": "vite preview",
    "build-only": "vite build --config  vite.config.ts",
    "build-lib": "vite build --config  vite.lib.config.ts",
    "build-es": "vite build --config  vite.es.config.ts",
    "build-cjs": "vite build --config  vite.cjs.config.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "@stophjc/hc-ui": "^1.0.2",
    "@types/lodash-es": "^4.17.12",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "async-validator": "^4.2.5",
    "axios": "^1.6.7",
    "lodash-es": "^4.17.21",
    "rollup": "^4.12.0",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "^1.0.0-rc.42",
    "vitest": "^1.2.2",
    "vue": "3.4.20",
    "vue-tsc": "^1.8.25"
  }
}
